{
  "name": "decsv",
  "productName": "DECSV",
  "version": "1.0.0",
  "description": "Document editor for CSV-files to download, edit, save and upload them.",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist-win": "electron-builder",
    "dist-multi": "electron-builder -mwl"
  },
  "build": {
    "appId": "com.squirrel.decsv.DECSV",
    "win": {
      "target": "squirrel",
      "icon": "./build/icon.ico"
    },
    "mac": {
      "category": "editor",
      "target": "default"
    },
    "squirrelWindows": {
      "iconUrl": "file:///./build/icon.ico",
      "remoteReleases": "https://github.com/Tyaisurm/testing",
      "loadingGif": "./build/install-spinner.gif"
    }
  },
  "homepage": "https://github.com/Tyaisurm/DECSV",
  "repository": "https://github.com/Tyaisurm/DECSV",
  "keywords": [
    "Electron",
    "DECSV"
  ],
  "author": {
    "name": "Jaakko Tuuri"
  },
  "licence": "MIT",
  "dependencies": {
    "electron-data": "2.1.0",
    "electron-log": "2.2.6",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "1.2.0",
    "jquery": "3.2.1",
    "knex": "0.13.0",
    "xlsx": "0.10.5",
    "sqlite3": "3.1.8"
  },
  "devDependencies": {
    "electron": "1.4.16",
    "electron-builder": "19.8.0",
    "electron-builder-squirrel-windows": "19.16.0"
  }
}
