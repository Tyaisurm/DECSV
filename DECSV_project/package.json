{
  "name": "decsv",
  "productName": "DECSV",
  "version": "1.2.0",
  "description": "SLIPPS survey results review and edit application.",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "pack-win": "electron-builder --win --ia32 --dir",
    "dist-win": "electron-builder --win --ia32",
    "dist-multi": "electron-builder -mwl"
  },
  "build": {
    "appId": "com.squirrel.decsv.DECSV",
    "win": {
      "target": "squirrel",
      "icon": "./build/icon.ico"
    },
    "mac": {
      "category": "editor",
      "target": "default"
    },
    "squirrelWindows": {
      "iconUrl": "https://github.com/Tyaisurm/DECSV/blob/master/DECSV_project/build/icon.ico?raw=true",
      "remoteReleases": "https://github.com/Tyaisurm/testing",
      "loadingGif": "./build/install-spinner.gif"
    }
  },
  "homepage": "https://github.com/Tyaisurm/DECSV",
  "repository": "https://github.com/Tyaisurm/DECSV",
  "keywords": [
    "Electron",
    "DECSV",
    "SLIPPS"
  ],
  "author": {
    "name": "Jaakko Tuuri"
  },
  "licence": "MIT",
  "dependencies": {
    "electron-log": "2.2.6",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "1.2.0",
    "electron-window-state": "4.1.1",
    "jquery": "3.2.1",
    "select2": "4.0.3",
    "xlsx": "0.10.5",
    "rimraf": "2.6.1"
  },
  "devDependencies": { 
    "electron": "1.4.16",
    "electron-builder": "19.16.2",
    "electron-builder-squirrel-windows": "19.16.0"
  }
}
